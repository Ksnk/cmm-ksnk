mod:=(evalasync('~user("Caption","номер модуля - 32=$20, 48=$30, 64=$40, 117=$75",300,200)'));


again:
setmask($f);
SendMess(mod*256+$ff,$8902);
if recvmess(a,code,p1,p2,p3,t1,t2)>0 then

write(mod,':',code , p1,p2,p3 , ' at ', t1,t2);
if code =  $0d07 then write('инициализация закончена') fi ;
if code =  $0d08 then write('рестарт верхнего модуля') fi ;
if code =  $0d0c then write('авария тракта') fi ;
if code =  $0d0d then write('восстановление тракта') fi ;
if code =  $0d10 then write('блокировка линии') fi ;

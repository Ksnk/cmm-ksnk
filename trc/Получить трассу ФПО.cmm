if code =  $0d12 then write('Потеря проключения') fi ;
if code =  $0d13 then write('смена выбора опорного тракта') fi ;
if code =  $0d1f then write('потерян модуль') fi ;
if code =  $0d21 then write('нет живых модулей по TP') fi ;
